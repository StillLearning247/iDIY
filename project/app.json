{
  "expo": {
    "name": "iDIY",
    "slug": "idiy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iDIY_logo2.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/iDIY_logo2.jpg"
    },
    "extra": {
      "env": {
        "SUPABASE_URL": "your-supabase-url",
        "SUPABASE_ANON_KEY": "your-anon-key"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.jdemers.idiy"
    }
  }
}
